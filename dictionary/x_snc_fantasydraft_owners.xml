<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Owners" max_length="40" name="x_snc_fantasydraft_owners" type="collection">
        <element label="Team Abbreviation" max_length="5" name="abbr" type="string"/>
        <element choice="1" label="Division" max_length="40" name="division" type="choice">
            <choice>
                <element label="Hearts" sequence="1" value="hearts"/>
                <element label="Clubs" sequence="2" value="clubs"/>
                <element label="Spades" sequence="3" value="spades"/>
                <element label="Diamonds" sequence="4" value="diamonds"/>
            </choice>
        </element>
        <element label="Owner Name" max_length="32" name="owner_name" reference="sys_user" type="reference"/>
        <element label="Team Name" max_length="100" name="team_name" type="string"/>
        <index name="index">
            <element name="owner_name"/>
        </index>
    </element>
</database>
